{
  "name": "@CryptoSwap/price-api-sdk",
  "version": "11.1.0",
  "description": "SDK for pancakeswap price api",
  "repository": {
    "type": "git",
    "url": "https://github.com/pancakeswap/pancake-frontend.git",
    "directory": "packages/price-api-sdk"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "update:snapshot": "vitest -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@CryptoSwap/permit2-sdk": "workspace:*",
    "@CryptoSwap/smart-router": "workspace:*",
    "@CryptoSwap/swap-sdk-core": "workspace:*",
    "@CryptoSwap/pcsx-sdk": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@CryptoSwap/permit2-sdk": {
      "optional": true
    },
    "@CryptoSwap/swap-sdk-core": {
      "optional": true
    },
    "@CryptoSwap/smart-router": {
      "optional": true
    },
    "@CryptoSwap/pcsx-sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@CryptoSwap/chains": "workspace:*"
  },
  "devDependencies": {
    "@CryptoSwap/permit2-sdk": "workspace:*",
    "@CryptoSwap/sdk": "workspace:*",
    "@CryptoSwap/smart-router": "workspace:*",
    "@CryptoSwap/swap-sdk-core": "workspace:*",
    "@CryptoSwap/pcsx-sdk": "workspace:*",
    "@CryptoSwap/tsconfig": "workspace:*",
    "tsup": "^6.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
